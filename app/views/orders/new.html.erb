<%= form_with(model: @order) do |f| %>
  <div>
    <label>Address</label>
    <%= f.text_field :address %>
  </div>
  <div>
    <label>Contact</label>
    <%= f.text_field :contact %>
  </div>
  <div>
    <label>Payment Info</label>
    <%= f.text_field :payment_info %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>